
<h1>Choose from your Company:</h1>


<ul>
  {{#each names}}

  <li>{{this}}</li>

  {{/each}}
</ul>


<article>
    <form method="POST" action="/getRandom">
        <div class="input-field">
            <label>Enter Company Name:</label>
            <input type="text" name="name" size="50" placeholder="Google">
        </div>

        <button type="submit">Submit </button>
    </form>
</article>
